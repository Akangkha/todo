{
    "version": 2,
    "name": "backend",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/chat",
        "dest": "/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      },
      {
        "src": "/(.*)",
        "dest": "/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      }
    ]
   
  }